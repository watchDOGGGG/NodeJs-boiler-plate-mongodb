{
  "name": "boiler-plate",
  "version": "0.1.0",
  "main": "server.ts",
  "repository": "",
  "license": "MIT",
  "dependencies": {
    "@types/amqplib": "0.10.1",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "amqplib": "0.10.3",
    "axios": "1.6.5",
    "bcryptjs": "2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "helmet": "7.1.0",
    "http-errors": "2.0.0",
    "husky": "8.0.3",
    "jsonwebtoken": "9.0.0",
    "lint-staged": "13.1.0",
    "mongoose": "^6.8.4",
    "morgan": "1.10.0",
    "nodemon": "3.0.2",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "winston": "3.10.0",
    "zod": "3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pnpm install && pnpm run build:clean && pnpm run build:compile",
    "build:qa": "pnpm install && pnpm run build:clean && pnpm run build:compile && rm -rf src",
    "build:clean": "pnpm run clean",
    "build:compile": "tsc",
    "clean": "rm -rf build",
    "dev": "nodemon --exec ts-node src/server.ts",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "start": "node build/src/server.js",
    "test": "jest --watch --runInBand"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/http-errors": "2.0.1",
    "@types/jsonwebtoken": "9.0.1",
    "@types/morgan": "1.9.7",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "^29.3.1",
    "prettier": "2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
